{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/analysis"
  ],
  "exclude": [
    "src/analysis/uint64",
    "src/analysis/test",
    "src/analysis/tmp",
  ]
}
